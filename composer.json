{
    "name": "mokhdesigns/langy",
    "description": "A Laravel package for multilingual models",
    "keywords": ["laravel", "translation", "language", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mokhtar Ali",
            "email": "me@devmokhtar.com",
            "homepage": "http://devmokhtar.com",
            "role": "Developer"
        }
    ],
    
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.8.*|6.*|7.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Mokhdesigns\\Langy\\": "src/Langy/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
      
                "Mokhdesigns\\Langy\\TranslatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
